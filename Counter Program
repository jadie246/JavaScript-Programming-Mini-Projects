let count = 0;

function increase() {
  count++;
  console.log("Count:", count);
}

function decrease() {
  count--;
  console.log("Count:", count);
}

function reset() {
  count = 0;
  console.log("Count:", count);
}

// Ví dụ chạy thử
increase(); // Count: 1
increase(); // Count: 2
decrease(); // Count: 1
reset();    // Count: 0
